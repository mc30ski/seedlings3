{
  "expo": {
    "name": "HelloWorldMobile",
    "slug": "hello-world-mobile",
    "scheme": "helloworld",
    "version": "1.0.0",
    "orientation": "portrait",
    "sdkVersion": "51.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "updates": {
      "enabled": false
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro"
    },
    "icon": "./assets/icon.png",
    "extra": {
      "EXPO_PUBLIC_API_BASE_URL": "http://localhost:8080"
    }
  }
}